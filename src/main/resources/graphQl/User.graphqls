type User{
    id:ID
    firstName:String
    lastName:String
    username:String
    email:String
    roleSet:[Role]
}


input UserInput{
    firstName:String
    lastName:String
    username:String
    email:String
    roleSet:[Role]
    password:String
}
extend type Mutation {
    createUser(userInput:UserInput):User
    updateUser(userInput:UserInput,id:ID):User
}