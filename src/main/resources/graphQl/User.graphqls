type User{
    id:ID
    firstName:String
    lastName:String
    username:String
    email:String
    rolesSet:[Roles]
}


input UserInput{
    firstName:String
    lastName:String
    username:String
    email:String
    rolesSet:[Roles]
    password:String
}
extend type Mutation {
    createUser(userInput:UserInput):User
    updateUser(userInput:UserInput,id:ID):User
}

extend type Query{
    userList:[User]
}