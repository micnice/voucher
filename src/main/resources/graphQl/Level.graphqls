type Level{
    id:ID
    name:String
    description:String

}
extend type Query {
levelList:[Level]
    levelById(id:ID):Level

}
input LevelInput{
    name:String
    description:String
}
extend type Mutation {
    createLevel(levelInput:LevelInput):Level
    updateLevel(levelInput:LevelInput,id:ID):Level
}